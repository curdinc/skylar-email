{
  "name": "@skylar/nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pnpm with-env next build",
    "clean": "git clean -xdf .next .turbo node_modules",
    "dev": "concurrently \"lt --port 3000 --subdomain skylar-frontend-$(grep DEV_NAME ../../.env | cut -d '=' -f2)\" \"pnpm with-env next dev\"",
    "lint": "dotenv -v SKIP_ENV_VALIDATION=1 next lint",
    "format": "prettier --check \"**/*.{js,cjs,mjs,ts,tsx,md,json}\" --ignore-path ../../.prettierignore",
    "start": "pnpm with-env next start",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@formkit/auto-animate": "^0.8.0",
    "@icons-pack/react-simple-icons": "^9.1.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-avatar": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.4",
    "@react-oauth/google": "^0.11.1",
    "@skylar/auth": "^0.1.0",
    "@skylar/client-db": "^0.1.0",
    "@skylar/gmail-api": "^0.1.0",
    "@skylar/logger": "^0.1.0",
    "@skylar/logic": "^0.1.0",
    "@skylar/parsers-and-types": "^0.1.0",
    "@skylar/tinykeys": "^0.1.0",
    "@stripe/react-stripe-js": "^2.3.0",
    "@stripe/stripe-js": "^2.1.5",
    "@tanstack/react-query": "^5.0.0",
    "@tanstack/react-query-devtools": "^5.0.3",
    "@tanstack/react-query-next-experimental": "5.0.0",
    "@trpc/client": "10.7.1-alpha-next-2023-10-20-20-37-31.88",
    "@trpc/next": "10.7.1-alpha-next-2023-10-20-20-37-31.88",
    "@trpc/react-query": "10.7.1-alpha-next-2023-10-20-20-37-31.88",
    "@trpc/server": "10.7.1-alpha-next-2023-10-20-20-37-31.88",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "dompurify": "^3.0.5",
    "lucide-react": "^0.288.0",
    "next": "^13.5.4",
    "next-axiom": "1.0.0-rc.1",
    "react": "18.2.0",
    "react-arborist": "^3.2.0",
    "react-dom": "18.2.0",
    "superjson": "1.13.1",
    "tailwind-merge": "^1.14.0"
  },
  "devDependencies": {
    "@skylar/api": "^0.1.0",
    "@skylar/eslint-config": "^0.2.0",
    "@skylar/prettier-config": "^0.1.0",
    "@skylar/tailwind-config": "^0.1.0",
    "@skylar/tsconfig": "^0.1.0",
    "@types/dompurify": "^3.0.3",
    "@types/node": "^18.17.13",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.15",
    "concurrently": "^8.2.1",
    "dotenv-cli": "^7.3.0",
    "encoding": "^0.1.13",
    "eslint": "^8.48.0",
    "localtunnel": "^2.0.2",
    "typescript": "^5.2.2"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@skylar/eslint-config/base",
      "@skylar/eslint-config/nextjs",
      "@skylar/eslint-config/react",
      "@skylar/eslint-config/react-bonus"
    ]
  },
  "prettier": "@skylar/prettier-config"
}
