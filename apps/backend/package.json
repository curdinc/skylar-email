{
  "name": "@skylar/backend",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "wrangler dev src/index.ts",
    "build": "wrangler deploy src/index.ts --dry-run --outdir dist",
    "clean": "rm -rf .turbo dist node_modules",
    "lint": "eslint .",
    "format": "prettier --check \"**/*.{mjs,ts,md,json}\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@hono/trpc-server": "^0.1.0",
    "@skylar/api": "^0.1.0",
    "@skylar/db": "^0.1.0",
    "@skylar/logger": "^0.1.0",
    "@skylar/schema": "^0.1.0",
    "hono": "^3.5.6"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230821.0",
    "@skylar/eslint-config": "0.2.0",
    "@skylar/prettier-config": "^0.1.0",
    "@skylar/tsconfig": "^0.1.0",
    "eslint": "^8.48.0",
    "typescript": "^5.2.2",
    "wrangler": "^3.6.0"
  },
  "eslintConfig": {
    "extends": [
      "@skylar/eslint-config/base"
    ]
  },
  "prettier": "@skylar/prettier-config"
}
