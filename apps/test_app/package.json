{
  "name": "@skylar/test_app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pnpm with-env next build",
    "clean": "git clean -xdf .next .turbo node_modules",
    "dev": "pnpm with-env next dev",
    "lint": "dotenv -v SKIP_ENV_VALIDATION=1 next lint",
    "format": "prettier --check \"**/*.{js,cjs,mjs,ts,tsx,md,json}\"",
    "start": "pnpm with-env next start",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@skylar/api": "^0.1.0",
    "@trpc/client": "^10.38.1",
    "next": "^13.4.19",
    "next-axiom": "1.0.0-rc.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "1.13.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@skylar/eslint-config": "^0.2.0",
    "@skylar/prettier-config": "^0.1.0",
    "@skylar/tailwind-config": "^0.1.0",
    "@skylar/tsconfig": "^0.1.0",
    "@types/node": "^18.17.13",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.48.0",
    "prettier": "^3.0.3",
    "tailwindcss": "3.3.2",
    "typescript": "^5.2.2"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@skylar/eslint-config/base",
      "@skylar/eslint-config/nextjs",
      "@skylar/eslint-config/react"
    ]
  },
  "prettier": "@skylar/prettier-config"
}