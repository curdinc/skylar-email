{
  "name": "@skylar/auth",
  "version": "0.1.0",
  "private": true,
  "main": "./server.ts",
  "types": "./server.ts",
  "license": "MIT",
  "exports": {
    ".": "./server.ts",
    "./client": "./client.ts",
    "./middleware": "./src/server/next-middleware.ts"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./server.ts"
      ],
      "client": [
        "./client.ts"
      ],
      "middleware": [
        "./src/server/next-middleware.ts"
      ]
    }
  },
  "scripts": {
    "clean": "rm -rf .turbo node_modules ./**/*/*.js ./**/*/*.d.ts ./**/*/*/*.jsx ./**/*/*/*.d.ts",
    "lint": "eslint .",
    "format": "prettier --check \"**/*.{mjs,ts,md,json}\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@skylar/parsers-and-types": "^0.1.0",
    "@supabase/auth-helpers-nextjs": "^0.7.4",
    "@supabase/auth-helpers-react": "^0.4.2",
    "@supabase/auth-helpers-shared": "^0.4.1",
    "@supabase/supabase-js": "^2.33.1",
    "@tsndr/cloudflare-worker-jwt": "^2.2.2"
  },
  "devDependencies": {
    "@skylar/eslint-config": "^0.2.0",
    "@skylar/prettier-config": "^0.1.0",
    "@skylar/tsconfig": "^0.1.0",
    "eslint": "^8.48.0",
    "hono": "^3.5.6",
    "next": "^13.4.19",
    "react": "18.2.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "hono": "^3.5.6",
    "next": "^13.4.19",
    "react": "^18.2.0"
  },
  "peerDependenciesMeta": {
    "hono": {
      "optional": true
    },
    "next": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@skylar/eslint-config/base",
      "@skylar/eslint-config/react"
    ]
  },
  "prettier": "@skylar/prettier-config"
}
